{
    "name": "noiselabs/zf-debug-utils",
    "description": "Console commands and other utilities for debugging ZF2 apps",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítor Brandão",
            "email": "vitor@noiselabs.org"
        }
    ],
    "require": {
        "php": ">5.5",
        "zendframework/zend-console": "~2.4",
        "zendframework/zend-modulemanager": "~2.4",
        "zendframework/zend-mvc":  "~2.4",
        "container-interop/container-interop": "^1.0"
    },
    "require-dev": {
      "fabpot/php-cs-fixer": "^1.11",
      "phpunit/phpunit": "^4.8"
    },
    "autoload": {
      "psr-4": {
        "Noiselabs\\ZfDebugModule\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Noiselabs\\ZfDebugModule\\": "test"
      }
    }
}
